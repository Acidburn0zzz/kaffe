prefix=		@prefix@
srcdir=		@srcdir@
incdir=		@includedir@/kaffe
INSTROOT=	@INSTROOT@
INSTALL=	@INSTALL@
INSTALL_DATA=	@INSTALL_DATA@
MKDIR=		@MKDIR@

KAFFEH=		kaffeh

SHELL=		/bin/sh

BUILT_INCLUDEFILES= \
		jtypes.h

INCLUDEFILES= \
		native.h \
		jmalloc.h \
		jsyscall.h \
		jni.h \
		Arrays.h \
		java_lang_Object.h \
		java_lang_String.h \
		java_lang_Throwable.h \
		java_lang_Thread.h \
		java_lang_ThreadGroup.h \

all: $(BUILT_INCLUDEFILES)

clean:

distclean:
		rm -f Makefile $(BUILT_INCLUDEFILES) .depend

install:
		test -d $(INSTROOT)$(incdir) || $(MKDIR) $(INSTROOT)$(incdir)
		for i in $(BUILT_INCLUDEFILES) ;\
		do \
			$(INSTALL_DATA) $$i $(INSTROOT)$(incdir)/$$i ;\
		done
		for i in $(INCLUDEFILES) ;\
		do \
			$(INSTALL_DATA) $(srcdir)/$$i $(INSTROOT)$(incdir)/$$i ;\
		done

derived-files:
		$(KAFFEH) java/lang/Thread
		$(KAFFEH) java/lang/ThreadGroup
		$(KAFFEH) java/lang/Throwable

depend:

