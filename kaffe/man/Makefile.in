prefix=		@prefix@
srcdir=		@srcdir@
mandir=		@mandir@
INSTROOT=	@INSTROOT@
INSTALL=	@INSTALL@
INSTALL_DATA=	${INSTALL} -m 644
MKDIR=		@MKDIR@

all:

clean:

distclean:
		rm -f Makefile .depend

install:
		test -d $(INSTROOT)$(mandir) || $(MKDIR) $(INSTROOT)$(mandir)
		test -d $(INSTROOT)$(mandir)/man1 || $(MKDIR) $(INSTROOT)$(mandir)/man1
		$(INSTALL_DATA) kaffe.1 $(INSTROOT)$(mandir)/man1/kaffe.1

derived-files:

depend:

