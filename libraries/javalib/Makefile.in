	   $(distdir)/kaffe/net/www/protocol/system \
	   $(distdir)/kaffe/text/dateformat \
	   $(distdir)/kaffe/text/numberformat $(distdir)/kaffe/util
	@for file in $(DISTFILES); do \
	  d=$(srcdir); \
	  if test -d $$d/$$file; then \
	    cp -pr $$/$$file $(distdir)/$$file; \
	  else \
	    test -f $(distdir)/$$file \
	    || ln $$d/$$file $(distdir)/$$file 2> /dev/null \
	    || cp -p $$d/$$file $(distdir)/$$file; \
	  fi; \
	done
