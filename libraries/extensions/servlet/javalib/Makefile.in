install-jrelibDATA: $(jrelib_DATA)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(jrelibdir)
	@list='$(jrelib_DATA)'; for p in $$list; do \
	  if test -f $(srcdir)/$$p; then \
	    echo " $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(jrelibdir)/$$p"; \
	    $(INSTALL_DATA) $(srcdir)/$$p $(DESTDIR)$(jrelibdir)/$$p; \
	  else if test -f $$p; then \
	    echo " $(INSTALL_DATA) $$p $(DESTDIR)$(jrelibdir)/$$p"; \
	    $(INSTALL_DATA) $$p $(DESTDIR)$(jrelibdir)/$$p; \
	  fi; fi; \
	done

uninstall-jrelibDATA:
	@$(NORMAL_UNINSTALL)
	list='$(jrelib_DATA)'; for p in $$list; do \
	  rm -f $(DESTDIR)$(jrelibdir)/$$p; \
	done
