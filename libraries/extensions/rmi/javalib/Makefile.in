	   $(distdir)/java/rmi/dgc $(distdir)/java/rmi/registry \
	   $(distdir)/java/rmi/server $(distdir)/kaffe/rmi/dgc \
	   $(distdir)/kaffe/rmi/registry $(distdir)/kaffe/rmi/rmic \
	   $(distdir)/kaffe/rmi/server
	@for file in $(DISTFILES); do \
	  d=$(srcdir); \
	  if test -d $$d/$$file; then \
	    cp -pR $$d/$$file $(distdir); \
	  else \
	    test -f $(distdir)/$$file \
	    || cp -p $$d/$$file $(distdir)/$$file || :; \
	  fi; \
	done
