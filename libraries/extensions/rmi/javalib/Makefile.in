install-classDATA: $(class_DATA)
	@$(NORMAL_INSTALL)
	$(mkinstalldirs) $(DESTDIR)$(classdir)
	@list='$(class_DATA)'; for p in $$list; do \
	  if test -f "$$p"; then d= ; else d="$(srcdir)/"; fi; \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  echo " $(INSTALL_DATA) $$d$$p $(DESTDIR)$(classdir)/$$f"; \
	  $(INSTALL_DATA) $$d$$p $(DESTDIR)$(classdir)/$$f; \
	done

uninstall-classDATA:
	@$(NORMAL_UNINSTALL)
	@list='$(class_DATA)'; for p in $$list; do \
	  f="`echo $$p | sed -e 's|^.*/||'`"; \
	  echo " rm -f $(DESTDIR)$(classdir)/$$f"; \
	  rm -f $(DESTDIR)$(classdir)/$$f; \
	done
